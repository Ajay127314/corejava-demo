public class FordTrucks implements Truck {
	
	int axles;
	double weight;
	
	
	

	public FordTrucks(int axles, double weight) {
		this.axles = axles;
		this.weight = weight;
	}

	@Override
	public int getAxles() {
		
		return axles;
	}

	@Override
	public double getWeight() {
		return weight;
	}

	@Override
	public String getBrand() {
		return "ford";
	}

}
