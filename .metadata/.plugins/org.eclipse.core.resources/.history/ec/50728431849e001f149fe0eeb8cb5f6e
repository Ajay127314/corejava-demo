public class Book {
	
	int id;
	String name,author,publisher;
	public int quantity;
	
	
	public Book(int id, String name, String author, String publisher,int quality) {
		this.id = id;
		this.name = name;
		this.author = author;
		this.publisher = publisher;
		this.quality = quality;
	}
	
	
	

}
